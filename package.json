{
  "name": "simplyimport",
  "version": "3.0.2",
  "description": "Export-less imports for JS/Coffee files featuring conditional, inline, & minified imports",
  "keywords": [
    "import",
    "importer",
    "jsimport",
    "jsimporter",
    "require",
    "browserify",
    "webpack",
    "include",
    "amd",
    "coffee-import"
  ],
  "main": "index.js",
  "bin": {
    "simplyimport": "./bin"
  },
  "author": "danielkalen",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielkalen/simplyimport"
  },
  "license": "MIT",
  "mocha_opts": "-u tdd --compilers coffee:coffee-script/register --slow 1000 --timeout 3000",
  "scripts": {
    "build": "npm run compile && npm run prependbin && npm run makeexec",
    "compile": "npm run compile:module && npm run compile:bin",
    "compile:module": "foreach -g 'lib/*' -x 'cat {{path}} | coffee -c -s -b > forCoverage/{{name}}.js'",
    "compile:bin": "coffee -o ./forCoverage -c -b lib/cli.coffee",
    "prependbin": "echo '#!/usr/bin/env node' | cat - forCoverage/cli.js > forCoverage/bin && rm -rf forCoverage/cli.js",
    "makeexec": "chmod +x forCoverage/bin",
    "coverage": "npm run build && forCoverage=true istanbul cover --dir test/coverage/node node_modules/mocha/bin/_mocha -- $npm_package_mocha_opts && npm run coverage:badge",
    "coverage:badge": "coffee .config/badge-gen.coffee",
    "test:debug": "mocha $npm_package_mocha_opts --debug-brk --inspect",
    "test": "mocha $npm_package_mocha_opts"
  },
  "dependencies": {
    "acorn": "^4.0.4",
    "array-includes": "^3.0.2",
    "bluebird": "^3.4.7",
    "chalk": "^1.1.3",
    "coffee-script": "^1.10.0",
    "escodegen": "^1.8.1",
    "extend": "^3.0.0",
    "fs-extra": "^0.30.0",
    "md5": "^2.1.0",
    "object.entries": "^1.0.4",
    "resolve": "^1.2.0",
    "string-replace-async": "^1.2.0",
    "uglify-js": "^2.6.1",
    "uniq": "^1.0.1",
    "yargs": "^3.29.0"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "create-output-stream": "0.0.1",
    "foreach-cli": "^1.6.0",
    "istanbul": "^0.4.5",
    "lcov-parse": "0.0.10",
    "mocha": "^3.1.2",
    "pretty-error": "^2.0.2",
    "request": "^2.74.0",
    "streamify-string": "^1.0.1",
    "svg2png": "^3.0.1",
    "timeunits": "^1.0.2"
  }
}
