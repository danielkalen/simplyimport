{
  "name": "simplyimport",
  "version": "4.0.0-g",
  "description": "Inline imports/exports for JS/Coffee featuring ES6 & CommonJS syntax support, transforms, conditional & duplicate imports, etc.",
  "keywords": [
    "import",
    "importer",
    "jsimport",
    "jsimporter",
    "require",
    "browserify",
    "webpack",
    "include",
    "amd",
    "coffee-import"
  ],
  "main": "index.js",
  "bin": {
    "simplyimport": "./bin",
    "simplyimport-debug": "./bin-debug"
  },
  "author": "danielkalen",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielkalen/simplyimport"
  },
  "license": "MIT",
  "mocha_opts": "-u tdd --compilers coffee:coffee-register --slow 1000 --timeout 8000",
  "scripts": {
    "prepublishOnly": "npm run test -s",
    "build": "npm run compile && npm run prependbin && npm run makeexec",
    "compile": "npm run compile:module && npm run compile:bin",
    "compile:module": "foreach -g 'lib/*' -x 'cat {{path}} | coffee -c -s -b > forCoverage/{{name}}.js'",
    "compile:bin": "coffee -o ./forCoverage -c -b lib/cli.coffee",
    "prependbin": "echo '#!/usr/bin/env node' | cat - forCoverage/cli.js > forCoverage/bin && rm -rf forCoverage/cli.js",
    "makeexec": "chmod +x forCoverage/bin",
    "coverage": "npm run build && npm run coverage:run && npm run coverage:badge",
    "coverage:run": "mocha $npm_package_mocha_opts --require coffee-coverage/register-istanbul; istanbul report",
    "coverage:badge": "badge-gen -d ./.config/badges/coverage",
    "test:debug": "mocha $npm_package_mocha_opts test/test.coffee --inspect --debug-brk",
    "test": "cake test"
  },
  "dependencies": {
    "@danielkalen/browserify-zlib": "^0.1.5",
    "acorn": "^5.0.3",
    "assert": "^1.4.1",
    "ast-types": "^0.9.11",
    "astravel": "^0.3.14",
    "astring": "^1.0.2",
    "astw": "^2.2.0",
    "bluebird": "^3.5.0",
    "browser-resolve": "^1.11.2",
    "browserify": "^14.4.0",
    "browserify-zlib": "^0.1.4",
    "buffer": "^5.0.2",
    "chalk": "^1.1.3",
    "coffee-register": "^0.1.0",
    "coffee-script": "^1.12.6",
    "coffeeify": "^2.1.0",
    "combine-source-map": "^0.8.0",
    "commander": "^2.10.0",
    "console-browserify": "^1.1.0",
    "constants-browserify": "^1.0.0",
    "convert-source-map": "^1.5.0",
    "crypto-browserify": "^3.11.0",
    "csonify": "^1.0.1",
    "debug": "^2.6.8",
    "decaffeinate-parser": "^13.1.1",
    "domain-browser": "^1.1.7",
    "events": "^1.1.1",
    "extend": "^3.0.0",
    "fs-jetpack": "^0.13.3",
    "get-stream": "^3.0.0",
    "https-browserify": "0.0.1",
    "lines-and-columns": "^1.1.6",
    "md5": "^2.1.0",
    "micromatch": "^2.3.11",
    "minimist": "^1.2.0",
    "os-browserify": "^0.2.1",
    "path-browserify": "0.0.0",
    "process": "^0.11.9",
    "promise-break": "^0.1.0",
    "punycode": "^2.0.1",
    "querystring-es3": "^0.2.1",
    "read-pkg-up": "^2.0.0",
    "scrumpy-es5": "^0.1.0",
    "stack-filter": "^1.0.1",
    "stream-browserify": "^2.0.1",
    "stream-http": "^2.5.0",
    "streamify-string": "^1.0.1",
    "string_decoder": "^0.10.31",
    "sugar": "^2.0.4",
    "syntax-error-plus": "^1.3.0",
    "through2": "^2.0.3",
    "timers-browserify": "^2.0.2",
    "treeify": "^1.0.1",
    "tsify-transform": "^1.0.0",
    "tty-browserify": "0.0.0",
    "umd": "^3.0.1",
    "url": "^0.11.0",
    "util": "^0.10.3",
    "vm-browserify": "0.0.4",
    "yamlify": "^0.1.3"
  },
  "devDependencies": {},
  "false": {}
}
