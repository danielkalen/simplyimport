{
  "name": "simplyimport",
  "version": "3.3.2",
  "description": "Inline imports/exports for JS/Coffee featuring ES6 & CommonJS syntax support, transforms, conditional & duplicate imports, etc.",
  "keywords": [
    "import",
    "importer",
    "jsimport",
    "jsimporter",
    "require",
    "browserify",
    "webpack",
    "include",
    "amd",
    "coffee-import"
  ],
  "main": "index.js",
  "bin": {
    "simplyimport": "./bin",
    "simplyimport-debug": "./bin-debug"
  },
  "author": "danielkalen",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielkalen/simplyimport"
  },
  "license": "MIT",
  "mocha_opts": "-u tdd --compilers coffee:coffee-script/register --slow 1000 --timeout 5000",
  "scripts": {
    "prepublish": "npm run test",
    "postinstall": "coffee scripts/installSampleModules.coffee",
    "build": "npm run compile && npm run prependbin && npm run makeexec",
    "compile": "npm run compile:module && npm run compile:bin",
    "compile:module": "foreach -g 'lib/*' -x 'cat {{path}} | coffee -c -s -b > forCoverage/{{name}}.js'",
    "compile:bin": "coffee -o ./forCoverage -c -b lib/cli.coffee",
    "prependbin": "echo '#!/usr/bin/env node' | cat - forCoverage/cli.js > forCoverage/bin && rm -rf forCoverage/cli.js",
    "makeexec": "chmod +x forCoverage/bin",
    "coverage": "npm run build && npm run coverage:run && npm run coverage:badge",
    "coverage:run": "forCoverage=true istanbul cover --dir coverage/ node_modules/mocha/bin/_mocha -- $npm_package_mocha_opts",
    "coverage:badge": "coffee .config/badge-gen.coffee",
    "test:debug": "mocha $npm_package_mocha_opts --debug-brk --inspect",
    "test": "mocha $npm_package_mocha_opts"
  },
  "dependencies": {
    "@danielkalen/browserify-zlib": "^0.1.5",
    "acorn": "^4.0.4",
    "array-includes": "^3.0.2",
    "assert": "^1.4.1",
    "bluebird": "^3.4.7",
    "browser-resolve": "^1.11.2",
    "browserify-zlib": "^0.1.4",
    "buffer": "^5.0.2",
    "chalk": "^1.1.3",
    "coffee-script": "^1.10.0",
    "concat-stream": "^1.6.0",
    "console-browserify": "^1.1.0",
    "constants-browserify": "^1.0.0",
    "crypto-browserify": "^3.11.0",
    "decaffeinate-parser": "^13.1.1",
    "domain-browser": "^1.1.7",
    "escodegen": "^1.8.1",
    "events": "^1.1.1",
    "extend": "^3.0.0",
    "fs-extra": "^0.30.0",
    "https-browserify": "0.0.1",
    "is-plain-obj": "^1.1.0",
    "md5": "^2.1.0",
    "micromatch": "^2.3.11",
    "minimist": "^1.2.0",
    "object.entries": "^1.0.4",
    "os-browserify": "^0.2.1",
    "path-browserify": "0.0.0",
    "process": "^0.11.9",
    "punycode": "^2.0.1",
    "querystring-es3": "^0.2.1",
    "read-pkg-up": "^2.0.0",
    "stack-filter": "^1.0.1",
    "stream-browserify": "^2.0.1",
    "stream-http": "^2.5.0",
    "streamify-string": "^1.0.1",
    "string-replace-async": "^1.2.0",
    "string_decoder": "^0.10.31",
    "syntax-error": "^1.3.0",
    "timers-browserify": "^2.0.2",
    "tty-browserify": "0.0.0",
    "uniq": "^1.0.1",
    "url": "^0.11.0",
    "util": "^0.10.3",
    "vm-browserify": "0.0.4",
    "yargs": "^3.29.0"
  },
  "devDependencies": {
    "axios": "^0.16.1",
    "babel-preset-es2015-script": "^1.1.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "create-output-stream": "0.0.1",
    "foreach-cli": "^1.6.0",
    "fs-jetpack": "^0.13.3",
    "istanbul": "^0.4.5",
    "jquery-selector-cache": "^1.0.2",
    "lcov-parse": "0.0.10",
    "mocha": "^3.1.2",
    "request": "^2.74.0",
    "smart-extend": "^1.3.2",
    "streamify-string": "^1.0.1",
    "svg2png": "^3.0.1",
    "tar.gz": "^1.0.5",
    "through2": "^2.0.3",
    "timeunits": "^1.0.2"
  }
}
